{
	"name": "Existing Docker Compose (Extend)",
	"dockerComposeFile": [
		"../compose.yaml"
	],
	"service": "app",
  "workspaceFolder": "/app",
	"shutdownAction": "stopCompose",
	// Configure tool-specific properties.
	"customizations": {
		"vscode": {
			"settings": {
				"editor.defaultFormatter": "biomejs.biome",
				"editor.formatOnSave": true
			},
			"extensions": [
				"biomejs.biome"
			]
		}
	}
}
